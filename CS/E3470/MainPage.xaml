<UserControl x:Class="E3470.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:dxp="http://schemas.devexpress.com/winfx/2008/xaml/printing"
    xmlns:local="clr-namespace:E3470"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400">

    <dxp:DocumentPreview AutoCreateDocument="True">
        <dxp:DocumentPreview.Model>
            <local:CustomReportServicePreviewModel ReportName="E3470.Web.XtraReport1, E3470.Web" ServiceUri="../ReportService1.svc"/>
        </dxp:DocumentPreview.Model>
    </dxp:DocumentPreview>
</UserControl>
